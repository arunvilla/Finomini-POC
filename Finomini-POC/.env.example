# AI Finance Manager Environment Variables
# Copy this file to .env and fill in your actual values

# Anthropic AI Configuration
VITE_ANTHROPIC_API_KEY=your_anthropic_api_key_here

# OpenAI Configuration (optional)
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Optional: Specify AI models
VITE_ANTHROPIC_MODEL=claude-3-haiku-20240307
VITE_OPENAI_MODEL=gpt-3.5-turbo

# Plaid Configuration (if using)
VITE_PLAID_CLIENT_ID=your_plaid_client_id
VITE_PLAID_SECRET=your_plaid_secret
VITE_PLAID_ENV=sandbox

# App Configuration
VITE_APP_NAME=AI Finance Manager
VITE_APP_VERSION=1.2.0